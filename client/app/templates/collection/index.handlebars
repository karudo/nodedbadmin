{{pagination-links curPageNum=pageNum pageSize=pageSize allCount=model.allCount}}
<table class="table table-bordered table-striped">
<thead>
    <tr>
        <th>&nbsp;</th>
        {{#each col in model.headers}}
            <th>{{col}}</th>
        {{/each}}
    </tr>
</thead>
<tbody>
{{#each row in acont}}
    <tr>
        <td>{{#link-to 'collection.edit' (query-params pk=row.pk)}}edit{{/link-to}}</td>
        {{#each col in row}}
            <td>{{col}}</td>
        {{/each}}
    </tr>
{{/each}}
</tbody>
</table>

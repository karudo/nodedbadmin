fs = require 'fs'
{join} = require 'path'
_s = require 'underscore.string'

classes = {}

files = fs.readdirSync join __dirname, '../mixins'
for f in files
  classObject = require join '../mixins', f
  className = _s.classify(f.split('.')[0])
  classes[className] = classObject

module.exports = classes
